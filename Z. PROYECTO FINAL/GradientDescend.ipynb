   "source": [
    "import torch\n",
    "\n",
    "\n",
    "A= torch.rand(5,5)\n",
    "x= torch.rand(5,1)\n",
    "\n",
    "y=A@x #a por x\n",
    "\n",
    "learning_rate=0.1\n",
    "\n",
    "def grad(x):\n",
    "    return 2*A.T*(A*x-y)\n",
    "\n",
    "for i in range(1000):\n",
    "    x=x-learning_rate*grad(x)\n",
    "\n",
    "print((A*x)-y)\n",
    "\n",
    "torch.clamp(x,0,1)\n",
    "print(x)\n",
    "\n",
    "    \n",
    "     "
